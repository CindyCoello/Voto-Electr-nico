@model VotoElectronico.Models.tbMovimientos

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*<h2>Create</h2>*@

@using (Html.BeginForm("Create", "Movimientos", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <br />
    <br />

    <fieldset>
        <div class="form-horizontal">
            <div class="form-group">

                <label class="control-label"></label>
                <div class="col-md-10">
                    <input type="file" name="LogoFile" class="form-control" />
                    @Html.ValidationMessageFor(model => model.Logofile, "", new { @class = "text-danger" })
                </div>
            </div>



            <div class="editor-label">
                @Html.LabelFor(model => model.part_Id)
            </div>
            <div class="editor-field">
                  @Html.DropDownList("part_Id", null, htmlAttributes: new { @class = "form-control col-md-6" })
                @Html.ValidationMessageFor(model => model.part_Id, "", new { @class = "text-danger" })
            </div>

            <div class="editor-label">
                @Html.LabelFor(model => model.mov_Nombre)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.mov_Nombre, new { htmlAttributes = new { @class = "form-control col-md-6" } })
                @Html.ValidationMessageFor(model => model.mov_Nombre)
            </div>


            <div class="editor-label">
                @Html.LabelFor(model => model.mov_Eslogan)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.mov_Eslogan, new { htmlAttributes = new { @class = "form-control col-md-6" } })
                @Html.ValidationMessageFor(model => model.mov_Eslogan)
            </div>



            <br />
            <br />
            <p>
                <input type="submit" value="Guardar" class="btn btn-outline-primary" />
            </p>

        </div>


    </fieldset>
}